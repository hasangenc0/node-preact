{
  "name": "preact-node",
  "version": "1.0.0",
  "description": "Node.js with preact frontend",
  "main": "server.js",
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack -p",
    "watch": "webpack --watch",
    "precommit": "NODE_ENV=production lint-staged",
    "styles": "./node_modules/.bin/tailwind build ./src/styles/styles.css -c ./tailwind.js -o ./public/css/app.css",
    "start": "nodemon server.js",
    "debug": "node --inspect-brk server.js",
    "test": "test",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hasangenc0/node-preact.git"
  },
  "keywords": [
    "preact",
    "nodejs",
    "express",
    "docker",
    "tailwind",
    "webpack",
    "babel",
    "pug"
  ],
  "author": "github.com/hasangenc0",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hasangenc0/node-preact/issues"
  },
  "homepage": "https://github.com/hasangenc0/node-preact#readme",
  "dependencies": {
    "express": "^4.18.2",
    "nodemon": "^1.18.9",
    "preact": "^8.4.2",
    "preact-compat": "^3.18.4",
    "pug": "^2.0.3",
    "webpack": "^4.28.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.12.0",
    "eslint-plugin-react": "^7.12.3",
    "tailwindcss": "^0.7.3",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack-cli": "^3.2.0"
  }
}
